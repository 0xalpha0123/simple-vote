{
  "name": "simplevotetutorial",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "testnetDefaultChannel": "v1.3.4",
  "scripts": {
    "_comment": "----------------------------",
    "update_docker": "solana-localnet update",
    "start_docker": "./start_docker",
    "stop_docker": "./stop_docker",
    "solana_logs": "solana-localnet logs -f",
    "localnet_up": "set -x; solana-localnet down; set -e; solana-localnet up",
    "localnet_down": "solana-localnet down",
    "slot": "babel-node src/client/slot.js",
    "keypair": "babel-node src/client/keypair.js",
    "balance": "babel-node src/client/balance.js",
    "airdrop": "babel-node src/client/airdrop.js",
    "dump": "babel-node src/client/dump.js",
    "build_simplest": "cargo update --manifest-path=src/simplest-rust/Cargo.toml && ./src/simplest-rust/do.sh build && rm -rf store/simplest.json",
    "test_simplest": "./src/simplest-rust/do.sh test",
    "clean_simplest": "./src/simplest-rust/do.sh clean && rm -rf store/simplest.json",
    "deploy_simplest": "babel-node src/client/deploy_simplest.js",
    "vote_simplest": "babel-node src/client/vote_simplest.js",
    "election_simplest": "babel-node src/client/election_simplest.js",
    "build_rejectdups": "cargo update --manifest-path=src/rejectdups-rust/Cargo.toml && ./src/rejectdups-rust/do.sh build && rm -rf store/rejectdups.json",
    "test_rejectdups": "./src/rejectdups-rust/do.sh test",
    "clean_rejectdups": "./src/rejectdups-rust/do.sh clean && rm -rf store/rejectdups.json",
    "deploy_rejectdups": "babel-node src/client/deploy_rejectdups.js",
    "vote_rejectdups": "babel-node src/client/vote_rejectdups.js",
    "count_rejectdups": "babel-node src/client/count_rejectdups.js",
    "ckrent_rejectdups": "babel-node src/client/ckrent_rejectdups.js",
    "clean_all": "./src/simplest-rust/do.sh clean && ./src/rejectdups-rust/do.sh clean && rm -rf store/simplest.json && rm -fr store/rejectdups.json",
    "cluster_local": "npm run clean_all && rm -fr .env",
    "cluster_devnet": "npm run clean_all && cp ./env/cluster-devnet.env .env",
    "cluster_testnet": "npm run clean_all && cp ./env/cluster-testnet.env .env",
    "cluster_testnet4XW": "npm run clean_all && cp ./env/cluster-testnet4XW.env .env",
    "cluster_mainnet": "npm run clean_all && cp ./env/cluster-mainnet-beta.env .env",
    "demo_caws": "babel-node src/client/demo_caws.js",
    "lint": "npm run pretty && eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "watch 'npm run lint:fix' . --wait=1",
    "flow": "flow",
    "flow:watch": "watch 'flow' . --wait=1 --ignoreDirectoryPattern=/doc/",
    "bpf-sdk:update": "solana-bpf-sdk-install node_modules/@solana/web3.js && npm run clean_all",
    "clean:store": "rm -rf store",
    "cluster:localnet": "npm run clean:store && rm -f .env",
    "cluster:devnet": "npm run clean:store && cp cluster-devnet.env .env",
    "cluster:testnet": "npm run clean:store && cp cluster-testnet.env .env",
    "cluster:mainnet-beta": "npm run clean:store && cp cluster-mainnet-beta.env .env",
    "localnet:update": "solana-localnet update",
    "localnet:up": "set -x; solana-localnet down; set -e; solana-localnet up",
    "localnet:down": "solana-localnet down",
    "localnet:logs": "solana-localnet logs -f",
    "pretty": "prettier --write '{,src/**/}*.js'",
    "postinstall": "npm run bpf-sdk:update && cargo update --manifest-path=src/simplest-rust/Cargo.toml"
  },
  "devDependencies": {
    "prettier": "^2.1.1"
  },
  "dependencies": {
    "@solana/web3.js": "^0.71.9",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bn.js": "^5.1.3",
    "body-parser": "^1.18.3",
    "buffer-layout": "^1.2.0",
    "css-loader": "^3.6.0",
    "dotenv": "8.2.0",
    "eslint": "^6.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.6",
    "event-emitter": "^0.3.5",
    "express": "^4.16.4",
    "flow-bin": "0.122.0",
    "flow-typed": "^3.2.1",
    "http-server": "^0.12.3",
    "jayson": "^3.3.4",
    "json-to-pretty-yaml": "^1.2.2",
    "mkdirp-promise": "^5.0.1",
    "moment": "^2.27.0",
    "mz": "^2.7.0",
    "node-fetch": "^2.2.0",
    "react": "^16.5.2",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.5.2",
    "readline-promise": "^1.0.3",
    "semver": "^7.3.2",
    "superstruct": "^0.8.4",
    "watch": "^1.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": "11.x"
  }
}
